{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-plusplus": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "no-use-before-define": ["error", { "functions": false, "classes": false }]
  }
}
